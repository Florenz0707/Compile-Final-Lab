void main() {
    const float RATE = 0.05;
    float principal = 1000.0;
    float interest;
    int time = 5;

    interest = principal * RATE * time;

    if (interest == 250.0) {
        float total = principal + interest;

        if (total <= 1250.0) {
            return;
        }
    }

    if (principal < 999.99 || time != 5) {
        return;
    }
}
